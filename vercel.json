{
    "version": 2,
    "builds": [
        {
            "src": "haltrack/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.12" 
                
            }
        }
    ],
    
   
    "buildCommand": "python manage.py collectstatic --noinput && vercel build",
    
    "routes": [
   
        {
            "src": "/(.*)",
            "dest": "haltrack/wsgi.py"
        }
    ]
}